{
  "name": "sb-admin-react",
  "version": "0.4.0",
  "description": "Starter theme for React JS Dashboard Apps.",
  "main": "src/main.jsx",
  "scripts": {
    "postinstall": "npm run build",
    "server": "node server.js",
    "build": "webpack -p --config webpack.production.config.js",
    "build-dev": "webpack -p --config webpack.local.config.js",
    "test": "PHANTOMJS_BIN=./node_modules/.bin/phantomjs ./node_modules/karma/bin/karma start karma.config.js",
    "test-cross-browser": "./node_modules/karma/bin/karma start karma.cross-browser.config.js",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "clean": "rm -f build/*.{js,ttf,eot,woff,woff2,svg}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ssteinerX/sb-admin-react.git"
  },
  "keywords": [
    "sb-admin",
    "bootstrap",
    "react",
    "skeleton",
    "template",
    "quickstart"
  ],
  "author": "Mark Fayngersh, Steve Steiner (ssteinerX)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ssteinerX/sb-admin-react/issues"
  },
  "homepage": "https://github.com/ssteinerX/sb-admin-react",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "bundle-loader": "^0.5.4",
    "chai": "^3.5.0",
    "coveralls": "^2.11.2",
    "css-loader": "^0.24.0",
    "express": "^4.12.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sauce-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.3",
    "karma-webpack": "^1.5.0",
    "less-loader": "^2.2.1",
    "mocha": "^3.0.2",
    "node-sass": "^3.4.1",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss-loader": "^0.11.0",
    "postcss-nested": "^1.0.0",
    "react-hot-loader": "^1.2.3",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "autoprefixer": "^6.0.2",
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.0.0",
    "classnames": "^2.2.0",
    "font-awesome": "^4.4.0",
    "jquery": "^3.1.0",
    "less": "^2.5.3",
    "nprogress": "^0.2.0",
    "react": "^15.2.1",
    "react-bootstrap": "^0.30.0",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0",
    "reflux": "^0.4.1"
  }
}
